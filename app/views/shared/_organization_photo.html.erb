<div>
    <ul class='img-dropdown'>
        <li class="dropdown">
            <% if current_organization.photo.attached? %>
                <%= cl_image_tag current_organization.photo.key,
                crop: :limit,
                class:"rounded-circle photo-user dropdwon-toggle",
                id:"imgDropdown",
                data: { bs_toggle: "dropdown"},
                'aria-haspopup': true,
                'aria-expanded': false %>
            <% else %>
                <%= image_tag "icon-user.png", class:"rounded-circle photo-user dropdown-toggle", id:'imgDropdown', data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
            <% end %>
            <div class="dropdown-menu dropdown-menu-end" aria-labelledby="imgDropdown">
                <%= link_to 'Settings', edit_organization_registration_path, class:'dropdown-item' %>
                <%= link_to "Log out", destroy_organization_session_path, data: {turbo_method: :delete}, class: "dropdown-item" %>
            </div>
        </li>
    </ul>
</div>
